{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A React Boilerplate that uses Webpack 3, ExpressJS, HMR, and Jest for unit testing",
  "main": "index.js",
  "author": "Shelby Hurns",
  "license": "MIT",
  "scripts": {
    "dev": "node index-server.js",
    "prod": "webpack --config webpack.config.prod.js",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.16.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "morgan": "^1.8.2",
    "node-sass": "^4.9.0",
    "prettier": "1.12.1",
    "raf": "^3.4.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^3.0.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/test_setup",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "dependencies": {
    "express": "^4.15.4",
    "identity-obj-proxy": "^3.0.0",
    "nodemon": "^1.12.0",
    "prop-types": "^15.5.10"
  }
}
